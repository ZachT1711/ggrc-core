{{#eq (attrDataType, 'Person')}}
  <tree-field-wrapper source:from="verifiers" type:from="verifierType" field:from="'email'">
    <tree-field source:from="items"/>
  </tree-field-wrapper>
{{else}}
  <span class="state-value-dot {{addclass 'state' reviewState}}">
    {{reviewState}}
  </span>
{{/eq}}
